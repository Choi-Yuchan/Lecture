
public class practice {
	public static void main (String[] args) {
		int 국어 = 79;
		int 영어 = 82;
		int 수학 = 79;
		
		int total = 국어 + 영어 + 수학;
		double average = total / 3;
		char grade;
		
		
		if (average > 90) {
			grade = '수';
		} else if (average > 80) {
			grade = '우';
		} else {
			grade = '미';
		}
				
		System.out.println("총점: " + total);
		System.out.println("평균: " + average);
		System.out.println("성적: " + grade);
	}
}
