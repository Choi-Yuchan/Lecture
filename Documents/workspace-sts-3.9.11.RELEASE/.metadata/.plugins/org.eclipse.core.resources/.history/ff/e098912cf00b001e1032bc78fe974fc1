
public class practice {
	public static void main (String[] args) {
		int num1, num2, num3;
		num1 = 16;
		num2 = 10;
		num3 = 1;
		int maxNumber;
		
		if((num1 > num2) && (num1 > num3)) {
			maxNumber = num1;
		} else if ((num2 > num1) && (num2 > num3)) {
			maxNumber = num2;
		} else if((num3 > num1) && (num3 > num2)) {
			maxNumber = num3;
		} else {
			maxNumber = 0;
		}
		
		System.out.println("최대값: " + maxNumber);
		
	}
}
