//Hello

public class HelloWorld2 {

	public static void main(String[] args) {
		short num = 10;
		num = (short)(num + 77L);
		int rate = 3;
		rate = (int)(rate * 3.5);
		System.out.println(num); //87
		System.out.println(rate); // 10
		
		num = 10;
		num += 77L;
		rate = 3;
		rate *= 3.5;
		System.out.println(num); // 87
		System.out.println(rate); // 10.5
		
//		System.out.println(count);
	}
}
